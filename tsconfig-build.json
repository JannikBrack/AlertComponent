{
  "extends": "./tsconfig.json",
  "include": ["lib"],
  "compilerOptions": {
    "declaration": true,
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  }
}